{
  "name": "ahem-server",
  "version": "1.0.0",
  "license": "LGPL-3",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"node ahem.js\" \"ng serve --proxy-config proxy.conf.json\"",
    "test": "ng test",
    "testServer": "node_modules/jest/bin/jest.js **/*/serverapp.spec.js",
    "code-coverage-badges": "node_modules/jest-coverage-badges/cli.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "npm install && npm rebuild node-sass && ng build",
    "buildDev": "npm install && npm rebuild node-sass && ng build --dev",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/universalServer --proxy-config proxy.conf.json",
    "build:client-and-server-bundles": "ng build --prod && ng run ahem-universal:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors --display-error-details",
    "webpack:backend": "webpack --config webpack.backend.config.js --progress --colors --display-error-details"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/server/app/*.js"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coverageDirectory": "./coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.8",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.8",
    "@angular/compiler": "^8.2.8",
    "@angular/core": "^8.2.8",
    "@angular/forms": "^8.2.8",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.8",
    "@angular/platform-browser-dynamic": "^8.2.8",
    "@angular/platform-server": "^8.2.8",
    "@angular/router": "^8.2.8",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "angular-swagger-ui": "^0.5.5",
    "angulartics2": "^7.2.4",
    "core-js": "^2.6.1",
    "express": "^4.15.2",
    "jsonwebtoken": "^8.4.0",
    "mailparser": "^2.4.3",
    "moment": "^2.23.0",
    "mongodb": "^3.1.10",
    "morgan": "^1.9.1",
    "ng2-adsense": "^5.4.0",
    "ngx-highlight-js": "^2.1.1",
    "ngx-moment": "^3.3.0",
    "nodemon": "^1.18.9",
    "rxjs": "^6.5.3",
    "smtp-server": "^3.4.7",
    "swagger-ui-express": "^4.0.2",
    "ts-loader": "^5.3.2",
    "winston": "^3.1.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.22",
    "@angular/cli": "^8.3.22",
    "@angular/compiler-cli": "^8.2.8",
    "@angular/language-service": "^8.2.8",
    "@types/jasmine": "^2.8.14",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.18",
    "codelyzer": "^5.0.1",
    "jasmine": "^3.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jasmine-ts": "^0.3.0",
    "jest": "^25.0.0",
    "jest-coverage-badges": "^1.0.0",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.4.0",
    "mongodb-memory-server": "^2.9.1",
    "node-sass": "^4.11.0",
    "nodemailer": "^4.7.0",
    "nyc": "^14.1.1",
    "protractor": "~5.4.1",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.1.2"
  }
}